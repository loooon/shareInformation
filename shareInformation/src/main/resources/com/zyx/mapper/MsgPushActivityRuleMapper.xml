<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zyx.mapper.MsgPushActivityRuleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zyx.model.MsgPushActivityRule">
        <result column="activity_rule_id" property="activityRuleId" />
        <result column="activity_rule_name" property="activityRuleName" />
        <result column="create_user_id" property="createUserId" />
        <result column="update_user_id" property="updateUserId" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="isEnable" property="isEnable" />
    </resultMap>
    <sql id="Base_Column_List">
    activity_rule_id,activity_rule_name,create_user_id,update_user_id
    ,create_time,update_time,isEnable
    </sql>

	<insert id="addRule">
	INSERT INTO msg_push_activity_rule 
	(activity_rule_id,activity_rule_name,create_user_id,update_user_id,create_time,update_time,isEnable) 
	VALUES
	(#{activityRuleId},#{activityRuleName},#{createUserId},#{updateUserId},now(),#{updateTime},'1')
	</insert>
</mapper>

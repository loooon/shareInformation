<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zyx.mapper.MsgPushAwardReceivingWayMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zyx.model.MsgPushAwardReceivingWay">
        <id column="receiving_way_id" property="receivingWayId" />
        <result column="receiving_way_name" property="receivingWayName" />
        <result column="create_user_id" property="createUserId" />
        <result column="start_time" property="startTime" />
        <result column="end_time" property="endTime" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>
	<sql id="Base_Column_List">
	receiving_way_id,receiving_way_name,create_user_id,start_time,end_time,create_time,update_time
	</sql>
	
	<insert id="addWay">
	INSERT INTO msg_push_award_receiving_way
	(receiving_way_id,receiving_way_name,create_user_id,start_time,end_time,create_time,update_time)
	VALUES
	(#{receivingWayId},#{receivingWayName},#{createUserId},#{startTime},#{endTime},now(),#{updateTime})
	</insert>
</mapper>
